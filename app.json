{
  "expo": {
    "name": "BaillaRock_Festival",
    "slug": "BaillaRock_Festival",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Dragon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.company.baillarockfestival", 
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/lea.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.company.baillarockfestival",  
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/lea.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
